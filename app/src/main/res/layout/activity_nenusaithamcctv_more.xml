<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tecdatum.iaca_tspolice.ViewData.ViewMoreData.NenuSaithamCCtvMore">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"

        android:theme="@style/AppTheme.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </com.google.android.material.appbar.AppBarLayout>

    <include layout="@layout/content_nenu_saitham_cctv_more" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_gravity="bottom|end"
        app:borderWidth="12dp"
        app:elevation="6dp"
        android:tint="#37448A"
        app:backgroundTint="#d9dbe1"
        android:scaleType="center"
        app:rippleColor="@android:color/black"
        app:srcCompat="@drawable/ho" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>
