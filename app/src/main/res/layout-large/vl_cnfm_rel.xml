<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:padding="5dp"
        android:text="More Details"
        android:background="@drawable/btn_b"
        android:textColor="#fff"
        android:textSize="14dp"
        android:gravity="center"
        android:textAppearance="?android:attr/textAppearanceMedium"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="10dp"
        android:layout_weight="0.09">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="0.09">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="PS Name" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_pscode"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />
            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#50504E"/>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="Worship Place" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_wp"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />
            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#50504E"/>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="Name of WorshipPlace" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_name_wp"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />



            </TableRow>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#50504E"/>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="Incharge Name" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_in"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />



            </TableRow>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#50504E"/>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="Phone Number  " />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_ph"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />





            </TableRow>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#50504E"/>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="Remarks " />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_dis"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />


            </TableRow>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:visibility="gone"
                android:background="#50504E"/>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:visibility="gone"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="Sensitive Level " />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_sp"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />


            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#50504E"/>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:padding="0dp">
                <TextView

                    android:layout_width="140dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:textColor="#302828"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:text="Address" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:text=":"/>

                <TextView
                    android:id="@+id/tv_religiou_add"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"
                    android:padding="5dp"
                    android:text=" "
                    android:layout_weight="0.3"
                    android:layout_marginLeft="11dp"
                    android:textColor="#3e2c2c"
                    android:textSize="12dp" />
            </TableRow>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#50504E"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ll"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:gravity="center"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btn_r_Confm"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:background="@drawable/btn_b"
                    android:text="Close"
                    android:textAllCaps="false"
                    android:textColor="#f7f8f9" />


            </LinearLayout>

        </LinearLayout>

    </ScrollView>
</LinearLayout>